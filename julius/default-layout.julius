$(function(){
    $("#login").hide().attr("title", "Log in to Haskellers").dialog({
        autoOpen: false,
        width: 500,
        modal: true
    });
    $("input[type=submit]").button();
    $("#login-button").click(function(){
        $("#login").dialog('open');
        return false;
    });
    $("#nav").css('height', $('html').height() + 'px');
});
